<?xml version="1.0" encoding="utf-8"?>

<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:androidx.coordinatorlayout.widget.CoordinatorLayoutxmlns="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:id="@+id/activity_allusers_coordinatorlayout"
    android:layout_height="match_parent"
    tools:context=".Activity_All_ServiceUser">


<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_All_ServiceUser">


    <include layout="@layout/toolbar"
        android:id="@+id/activity_allusers_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"/>
    <ProgressBar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="?android:attr/progressBarStyleHorizontal"
        android:id="@+id/activity_allusers_progressbar"
        android:indeterminate="true"
        android:backgroundTint="@color/colorAccent"
        android:layout_below="@id/activity_allusers_toolbar"/>
    <androidx.recyclerview.widget.RecyclerView
        android:layout_below="@+id/activity_allusers_toolbar"
        android:id="@+id/activity_allusers_recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>